{
	"version": "3.1.6",
	"private": true,
	"name": "vue-gh-pages",
	"description": "Tutorial to publish a public repository made with Vue in GithubPages.",
	"author": "beatrizsmerino@gmail.com",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "https://github.com/beatrizsmerino/vue-gh-pages.git"
	},
	"homepage": "https://beatrizsmerino.github.io/vue-gh-pages",
	"bugs": {
		"url": "https://github.com/beatrizsmerino/vue-gh-pages/issues"
	},
	"keywords": [
		"vue",
		"javascript",
		"github-pages",
		"tutorial"
	],
	"main": "index.js",
	"scripts": {
		"install:clean": "rm -rf node_modules package-lock.json",
		"postinstall": "npm run lint",
		"prepare": "husky",
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"prettier:fix": "prettier --write \"./**/*.{css,scss,sass,json,js,cjs,mjs,vue}\"",
		"eslint:fix": "vue-cli-service lint \"**/*.{json,js,cjs,mjs,vue}\" --fix --ignore-path .eslintignore",
		"stylelint:fix": "stylelint \"**/*.{css,scss,sass,vue}\" --fix --ignore-path .stylelintignore",
		"lint": "npm run prettier:fix && npm run eslint:fix && npm run stylelint:fix",
		"deploy:v1": "chmod +x ./deploy-v1.sh && ./deploy-v1.sh",
		"deploy:v2": "node deploy-v2.mjs"
	},
	"dependencies": {
		"chalk": "^5.6.0",
		"core-js": "^3.47.0",
		"execa": "^9.6.0",
		"node-emoji": "^2.2.0",
		"svg-symbol-sprite-loader": "^5.1.0",
		"vue": "^2.7.16",
		"vue-router": "^3.6.5"
	},
	"devDependencies": {
		"@babel/core": "^7.28.5",
		"@babel/eslint-parser": "^7.28.5",
		"@commitlint/cli": "^20.1.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@vue/cli-plugin-babel": "^5.0.9",
		"@vue/cli-plugin-eslint": "^5.0.9",
		"@vue/cli-plugin-router": "^5.0.9",
		"@vue/cli-service": "^5.0.9",
		"@vue/eslint-config-prettier": "^10.2.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-jsonc": "^2.21.0",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-vue": "^10.6.1",
		"eslint-webpack-plugin": "^5.0.2",
		"husky": "^9.1.7",
		"postcss": "^8.5.6",
		"postcss-html": "^1.8.0",
		"prettier": "^3.6.2",
		"sass": "^1.94.2",
		"sass-loader": "^16",
		"stylelint": "^16.26.0",
		"stylelint-config-property-sort-order-smacss": "^10.0.0",
		"stylelint-config-recommended-vue": "^1.6.1",
		"stylelint-config-sass-guidelines": "^12.1.0",
		"stylelint-config-standard": "^39.0.1",
		"stylelint-scss": "^6.12.1",
		"stylelint-webpack-plugin": "^5.0.1",
		"vue-template-compiler": "^2.7.16"
	},
	"engines": {
		"node": "20.18.0",
		"npm": "10.8.2"
	},
	"overrides": {
		"cross-spawn": "^7.0.6",
		"webpack-dev-server": "^5.2.1",
		"yorkie": "^2.0.0",
		"@vue/component-compiler-utils": {
			"postcss": "^8.4.47"
		},
		"vue-loader": {
			"@vue/component-compiler-utils": {
				"postcss": "^8.4.47"
			}
		}
	}
}
