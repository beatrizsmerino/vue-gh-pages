{
	"name": "vue-gh-pages",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"deploy": "chmod +x ./deploy.sh && ./deploy.sh"
	},
	"dependencies": {
		"core-js": "^3.15.1",
		"vue": "^2.6.14",
		"vue-router": "^3.5.2"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^4.5.13",
		"@vue/cli-plugin-eslint": "^4.5.13",
		"@vue/cli-service": "^4.5.13",
		"babel-eslint": "^10.1.0",
		"eslint": "^7.29.0",
		"eslint-plugin-vue": "^7.11.1",
		"node-sass": "^4.14.1",
		"sass-loader": "^10.1.1",
		"vue-template-compiler": "^2.6.14"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	]
}
