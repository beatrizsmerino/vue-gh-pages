{
	"version": "0.1.0",
	"private": true,
	"name": "vue-gh-pages",
	"description": "Tutorial to publish a public repository made with Vue in GithubPages.",
	"author": "beatrizsmerino@gmail.com",
	"license": "ISC",
	"main": "index.js",
	"scripts": {
		"install:clean": "rm -rf node_modules package-lock.json",
		"dependencies:fix": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions",
		"preinstall": "npm run install:clean && npm run dependencies:fix",
		"postinstall": "npm run lint",
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"prettier:fix": "prettier --write \"./**/*.{vue,js,json}\"",
		"eslint:fix": "vue-cli-service lint \"**/*.{json,js,vue}\" --fix --ignore-path .eslintignore",
		"stylelint:fix": "stylelint \"**/*.{css,scss,sass,vue}\" --fix --ignore-path .stylelintignore",
		"lint": "npm run prettier:fix && npm run eslint:fix && npm run stylelint:fix",
		"deploy": "node deploy.mjs"
	},
	"dependencies": {
		"core-js": "^3.32.0",
		"execa": "^7.2.0",
		"svg-symbol-sprite-loader": "^5.1.0",
		"vue": "^2.7.14",
		"vue-router": "^3.6.5"
	},
	"devDependencies": {
		"@babel/core": "^7.22.17",
		"@babel/eslint-parser": "^7.22.11",
		"@vue/cli-plugin-babel": "^5.0.8",
		"@vue/cli-plugin-eslint": "^5.0.8",
		"@vue/cli-plugin-router": "^5.0.8",
		"@vue/cli-service": "^5.0.8",
		"@vue/eslint-config-prettier": "^7.1.0",
		"eslint": "^8.46.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-vue": "^9.17.0",
		"eslint-webpack-plugin": "^4.0.1",
		"postcss": "^8.4.27",
		"postcss-html": "^1.5.0",
		"prettier": "^2.8.8",
		"sass": "^1.64.2",
		"sass-loader": "^10.4.1",
		"stylelint": "^15.10.2",
		"stylelint-config-property-sort-order-smacss": "^9.1.0",
		"stylelint-config-recommended-vue": "^1.5.0",
		"stylelint-config-sass-guidelines": "^10.0.0",
		"stylelint-config-standard": "^34.0.0",
		"stylelint-scss": "^5.0.1",
		"stylelint-webpack-plugin": "^4.1.1",
		"vue-template-compiler": "^2.7.14"
	},
	"resolutions": {},
	"volta": {
		"node": "14.21.3",
		"npm": "6.14.18"
	}
}
